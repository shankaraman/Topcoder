#include<iostream>
#include<string>
#include<vector>
using namespace std;
class ChatTranscript
{
    public:
        int howMany(vector <string> transcript,string name)
        {
            int count=0;
            int i;
            for (i=0;i<transcript.size();i++)
            {
                if(transcript[i].find(name+":")==0)
                {
                    count++;
                }
            }
         }
 };
